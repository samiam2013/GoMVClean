/*ELHO these are the JSON table schemas

THIS CAN AND NEEDS TO BE MADE PRIVATE by
  hiding this link from being served by the controller
    in controller.go*/

{ /* TABLE SCHEMAS */

"*localhost:8080*/model/public/schema" : {

    "schemaFile" : "schema.json",

    "SmallSchemaLang" : "*(read-only (recursion func())) jsonSettings *me "

},

"*localhost:8080*/model/public/updateStream" : {

    "schemaFile" : "schema.json",

    "SmallSchemaLang" : "httpLink : { (*read-write (schemaFile)) , (httpLink) }"

},

"*localhost:8080/model/public/modelRead" : {

    "schemaFile" : "schema.json",

    "SmallSchemaLang" : "httpLink : { (read-only (schemaFile)) , (httpLink) }"
}
